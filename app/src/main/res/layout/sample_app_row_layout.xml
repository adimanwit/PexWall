<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/phone_background"
        android:layout_width="@dimen/icon_background_size"
        android:layout_height="@dimen/icon_background_size"
        android:layout_marginBottom="@dimen/margin_extra_extra_small"
        android:translationZ="2dp"
        android:src="@drawable/background_round_blue"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="@+id/textView2" />

    <ImageView
        android:layout_width="@dimen/icon_size"
        android:layout_height="@dimen/icon_size"
        android:src="@drawable/ic_phone"
        android:translationZ="2dp"
        app:layout_constraintBottom_toBottomOf="@id/phone_background"
        app:layout_constraintEnd_toEndOf="@id/phone_background"
        app:layout_constraintStart_toStartOf="@id/phone_background"
        app:layout_constraintTop_toTopOf="@id/phone_background"
        app:tint="@color/darkGray" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_small"
        android:text="@string/phone"
        android:translationZ="2dp"
        android:textSize="@dimen/icon_text_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <ImageView
        android:id="@+id/message_background"
        android:layout_width="@dimen/icon_background_size"
        android:layout_height="@dimen/icon_background_size"
        android:translationZ="2dp"
        android:src="@drawable/background_round_blue"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:tint="@color/lightGray" />

    <ImageView
        android:layout_width="@dimen/icon_size"
        android:layout_height="@dimen/icon_size"
        android:translationZ="2dp"
        android:src="@drawable/ic_message"
        app:layout_constraintBottom_toBottomOf="@id/message_background"
        app:layout_constraintEnd_toEndOf="@id/message_background"
        app:layout_constraintStart_toStartOf="@id/message_background"
        app:layout_constraintTop_toTopOf="@id/message_background"
        app:tint="@color/darkGray" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/message"
        android:textSize="@dimen/icon_text_size"
        android:translationZ="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/clock_background"
        app:layout_constraintStart_toEndOf="@id/phone_background" />

    <ImageView
        android:id="@+id/clock_background"
        android:layout_width="@dimen/icon_background_size"
        android:layout_height="@dimen/icon_background_size"
        android:layout_marginBottom="@dimen/margin_extra_extra_small"
        android:src="@drawable/background_round_blue"
        android:translationZ="2dp"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:tint="@color/green" />

    <ImageView
        android:layout_width="@dimen/icon_size"
        android:layout_height="@dimen/icon_size"
        android:src="@drawable/ic_clock"
        android:translationZ="2dp"
        app:layout_constraintBottom_toBottomOf="@id/clock_background"
        app:layout_constraintEnd_toEndOf="@id/clock_background"
        app:layout_constraintStart_toStartOf="@id/clock_background"
        app:layout_constraintTop_toTopOf="@id/clock_background"
        app:tint="@color/darkGray" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/clock"
        android:textSize="@dimen/icon_text_size"
        app:layout_constraintBottom_toBottomOf="parent"
        android:translationZ="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/calendar_background"
        android:layout_width="@dimen/icon_background_size"
        android:layout_height="@dimen/icon_background_size"
        android:layout_marginBottom="@dimen/margin_extra_extra_small"
        android:src="@drawable/background_round_blue"
        android:translationZ="2dp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:tint="@color/yellow" />

    <ImageView
        android:layout_width="@dimen/icon_size"
        android:layout_height="@dimen/icon_size"
        android:src="@drawable/ic_calendar"
        android:translationZ="2dp"
        app:layout_constraintBottom_toBottomOf="@id/calendar_background"
        app:layout_constraintEnd_toEndOf="@id/calendar_background"
        app:layout_constraintStart_toStartOf="@id/calendar_background"
        app:layout_constraintTop_toTopOf="@id/calendar_background"
        app:tint="@color/darkGray" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/calendar"
        android:textSize="@dimen/icon_text_size"
        android:translationZ="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/map_textview"
        app:layout_constraintStart_toEndOf="@+id/textView3" />

    <ImageView
        android:id="@+id/map_background"
        android:layout_width="@dimen/icon_background_size"
        android:layout_height="@dimen/icon_background_size"
        android:layout_marginBottom="@dimen/margin_extra_extra_small"
        android:src="@drawable/background_round_blue"
        android:translationZ="2dp"
        app:layout_constraintBottom_toTopOf="@+id/map_textview"
        app:layout_constraintEnd_toEndOf="@id/map_textview"
        app:layout_constraintStart_toStartOf="@+id/map_textview"
        app:tint="@color/red" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="@dimen/icon_size"
        android:layout_height="@dimen/icon_size"
        android:src="@drawable/ic_map"
        android:translationZ="2dp"
        app:layout_constraintBottom_toBottomOf="@id/map_background"
        app:layout_constraintEnd_toEndOf="@id/map_background"
        app:layout_constraintStart_toStartOf="@id/map_background"
        app:layout_constraintTop_toTopOf="@id/map_background"
        app:tint="@color/darkGray" />

    <TextView
        android:id="@+id/map_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/margin_small"
        android:text="@string/map"
        android:translationZ="2dp"
        android:textSize="@dimen/icon_text_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>